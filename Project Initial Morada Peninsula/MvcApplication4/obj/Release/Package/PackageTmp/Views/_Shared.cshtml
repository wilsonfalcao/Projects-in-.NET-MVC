<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>.:Morada da Peninsula:.</title>
    <!-- Tell the browser to be responsive to screen width -->
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <!-- Bootstrap 3.3.5 -->
    <link rel="stylesheet" href="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css">
    <!-- Font Awesome -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.min.css">
    <!-- Theme style -->
    <link rel="stylesheet" href="~/Content/css/AdminLTE.min.css">
    <!-- AdminLTE Skins. Choose a skin from the css/skins
         folder instead of downloading all of them to reduce the load. -->
    <link rel="stylesheet" href="~/Content/css/_all-skins.min.css">
  </head>
  <body class="hold-transition skin-blue sidebar-mini">
    <div class="wrapper">
      <header class="main-header">
        <!-- Logo Entrada -->
        <a href="@Url.Action("Tela_RP","Login")" class="logo">
          <!-- mini logo for sidebar mini 50x50 pixels -->
          <span class="logo-mini"><b>MDP</b></span>
          <!-- logo for regular state and mobile devices -->
          <span class="logo-lg"><b>MDP </b>PENINSULA</span>
        </a>
        <!-- Barra Inicial Superior da Pagina  -->
        <nav class="navbar navbar-static-top" role="navigation">
          <!-- Sidebar toggle button-->
          <a href="#" class="sidebar-toggle" data-toggle="offcanvas" role="button">
            <span class="sr-only">Toggle navigation</span>
          </a>
            <div class="navbar-custom-menu">
            <ul class="nav navbar-nav">
            <li>
                <a href="#" data-toggle="control-sidebar"><i class="fa fa-gears"></i></a>
              </li>
            </ul>
            </div>
        </nav>
      </header>
      <!-- Painel de Link a Esquerda (1) -->
      <aside class="main-sidebar">
        <!-- sidebar: style can be found in sidebar.less -->
        <section class="sidebar">
          <!-- Painel de Identificação do usuário -->
          <div class="user-panel">
            <div class="pull-left image">
              <img src="~/imagem/user.jpg" class="img-circle" alt="User Image">
            </div>
            <div class="pull-left info">
              <p>@ViewBag.Nome_User</p>
              <a href="@Url.Action("Logout","Login")"><i class="fa fa-circle text-danger"></i> Lougout</a>
              <a href="@Url.Action("Settings","Login")"><i class="fa fa-circle text-success"></i>Settings</a>
            </div>
          </div>
          <!-- Painel geral do site -->
          <ul class="sidebar-menu">
            <li class="header">Painel de Navegação</li>
              <!--Links de Serviços-->
            <li class="active treeview">
              <a href="#">
                <i class="fa fa-user"></i> <span>Administrativos</span> <i class="fa fa-angle-left pull-right"></i>
              </a>
              <ul class="treeview-menu">
                <li class="active"><a href="@Url.Action("Cadastro_Usuario","Servicos")"><i class="fa fa-circle-o"></i> Criar User</a></li>
                <li><a href="@Url.Action("Cadastro_Categoria","Servicos")"><i class="fa fa-circle-o"></i>Criar Categoria</a></li>
                <li><a href="@Url.Action("Consultar_User","Servicos")"><i class="fa fa-circle-o"></i>Consultar Usuários</a></li>
                  
              </ul>
            </li>
            <li class="treeview">
              <a href="#">
                <i class="fa fa-comment"></i>
                <span>Diario de Bordo</span>
                <i class="fa fa-angle-left pull-right"></i>
              </a>
              <ul class="treeview-menu">
                <li><a href="@Url.Action("Diario_Bordo","Diario")"><i class="fa fa-circle-o"></i>Novo/Criar Diário</a></li>
                <li><a href="@Url.Action("Diario_Bordo_C","Diario")"><i class="fa fa-circle-o"></i>Consultar/Excluír Diário</a></li>
              </ul>
            </li>
            <li class="treeview">
              <a href="pages/widgets.html">
                <i class="fa fa-bullhorn"></i> <span>E-mail</span> <i class="fa fa-angle-left pull-right"></i>
              </a>
                <ul class="treeview-menu">
                <li><a href="@Url.Action("Cadastro_Email","Servicos")"><i class="fa fa-circle-o"></i>Cadastrar E-mail Novo</a></li>
                <li><a href="@Url.Action("Consultar_User","Servicos")"><i class="fa fa-circle-o"></i>Consultar</a></li>
                </ul>
            </li>
            <li class="treeview">
              <a href="#">
                <i class="fa fa-usd"></i>
                <span>Finanças</span>
                <i class="fa fa-angle-left pull-right"></i>
              </a>
              <ul class="treeview-menu">
                <li><a href="pages/charts/chartjs.html"><i class="fa fa-circle-o"></i> ChartJS</a></li>
                <li><a href="pages/charts/morris.html"><i class="fa fa-circle-o"></i> Morris</a></li>
                <li><a href="pages/charts/flot.html"><i class="fa fa-circle-o"></i> Flot</a></li>
                <li><a href="pages/charts/inline.html"><i class="fa fa-circle-o"></i> Inline charts</a></li>
              </ul>
            </li>
            <li><a href="documentation/index.html"><i class="fa fa-book"></i> <span>Documentação</span></a></li>
          </ul>
        </section>
        <!-- /.sidebar -->
      </aside>

      <!-- Área de Conteúdo (1) -->
      <div class="content-wrapper">
      <!-- Cabeçalho (1) -->
        <section class="content-header">
          <h1>
            Principal
            <small>Área de Visualização</small>
          </h1>
          <ol class="breadcrumb">
            <li><a href="#"><i class="fa fa-dashboard"></i> Home</a></li>
            <li class="active">Dashboard</li>
          </ol>
        </section>

      <!-- Conteúdo Principal -->
        <section class="content">
        @RenderBody()
        </section>
      <!-- /.Conteúdo -->
      </div>
      <!-- /.Área de Conteúdo -->

      <!-- Control Sidebar -->
      <aside class="control-sidebar control-sidebar-dark">
        <!-- Create the tabs -->
        <ul class="nav nav-tabs nav-justified control-sidebar-tabs">
          <li><a href="#control-sidebar-settings-tab" data-toggle="tab"><i class="fa fa-gears"></i></a></li>
        </ul>
        <!-- Tab panes -->
        <div class="tab-content">
          <!-- Home tab content -->
          <div class="tab-pane" id="control-sidebar-home-tab">
          </div>
           <!-- /.tab-pane -->
        </div>
      </aside>
      <!-- /.control-sidebar -->

      <!--Roda Pé-->
      <footer class="main-footer">
        <div class="pull-right hidden-xs">
          <b>Versão</b> 0.0.1
        </div>
        <strong>Copyright &copy;RD7 Tecnologia.</strong> Todos os Direitos Reservados.
      </footer>
      <div class="control-sidebar-bg"></div>
    </div>
      <!-- ./wrapper -->
  </body>
  <!--Bibliotecas-->
  <!-- jQuery 1.11.1 -->
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.0/jquery.min.js"></script>
  <!-- jQuery UI 1.11.4 -->
  <script src="~/Content/js/query-ui.min.js"></script>
  <!-- Resolve conflict in jQuery UI tooltip with Bootstrap tooltip -->
  <!-- Bootstrap 3.3.5 -->
  <script src="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/js/bootstrap.min.js"></script>
  <!-- Bootstrap WYSIHTML5 -->
  <script src="~/Content/js/bootstrap3-wysihtml5.all.min.js"></script>
  <!-- AdminLTE App -->
  <script src="~/Content/js/app.min.js"></script>
  <!-- AdminLTE dashboard demo (This is only for demo purposes) -->
  <script src="~/Content/js/dashboard.js"></script>
  <!-- AdminLTE for demo purposes -->
  <script src="~/Content/js/demo.js"></script>
</html>
