    <!--Section da Galeria-->
    <section id="lts_sec">
        <div class="container">
            <div class="row">
                <div class="col-lg-12 col-md-12 col-sm-12 col-xs12 ">
                    <div class="title_sec">
                        <h1>Galeria de Imagem</h1>
                        <h2>Nosso de registro de atividades</h2>
                    </div>
                </div>
                <div class="campo">
                    <div class="campo_inner">
                        <!-- Gallery -->
                        <section class="gallery">
                            @foreach (var Dados in Model.S5_Galeria as IEnumerable<Projeto_Laboral.Models.exib_gallery>)
                            {
                            if(Dados.type == 1)
                            {
                            <div class="gallery_item">
                            <span class="gallery_item_preview">
                            <a href="#" data-js="@Dados.id">
                            <svg fill="#f55" class="gallery_top" viewbox="0 3 60 20">
                            <path d="M 0.65359477,1.3817905 C 60.201925,8.44316 121.92863,11.583451 175.81699,28.832771 l 0.6536,-28.10457531 z"></path>
                            </svg>
                            <img src="@Dados.img" alt="" /><span>
                            <h3>@Dados.title</h3>
                            <p>@Dados.text</p>
                            </span>
                            </a>
                            </span>
                            <div data-lk="@Dados.id" class="gallery_item_full">
                            <div class="box">
                            <img src="@Dados.img" alt="" />
                            <h3>@Dados.title</h3>
                            <p>@Dados.text</p>
                            </div>
                            </div>
                            </div>
                            }
                            else
                            {
                            
                                 <!-- Gallery  item -->
      <div class="gallery_item">
        <!-- Gallery  item preview -->
        <span class="gallery_item_preview">
          <a href="#" data-js="@Dados.id">
            <svg fill="#5690F7" class="gallery_top" viewbox="0 3 60 20">
              <path d="M 0.65359477,1.3817905 C 60.201925,8.44316 121.92863,11.583451 175.81699,28.832771 l 0.6536,-28.10457531 z"></path>
            </svg>
            <img src="@Dados.img" alt="" /><span>
            <h3>@Dados.title</h3>
            <p>@Dados.text</p>

            </span></a>
        </span>
        <!-- Gallery  item full -->
        <div data-lk="@Dados.id" class="gallery_item_full">
          <div class="box">

            <div class="video">
              <iframe src="@Dados.video"></iframe>
            </div>

            <h3>@Dados.title</h3>
            <p>@Dados.text</p>
          </div>
        </div>
      </div>
                                
                            }
                            }
                        </section>
                    </div>
                </div>
                <div class="post_btn">
                    <div class="hover_effect_btn" id="hover_effect_btn">
