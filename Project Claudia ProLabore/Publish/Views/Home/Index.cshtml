    <h3>@Dados.title</h3>
        @if(Dados.text.Length > 20)
        {
        <p>@Dados.text.Substring(0,20)</p>
        }
        else
        {
        <p>@Dados.text</p>
        }
    </span>
    </a>
    </span>
        </div>
                             <div class="modal fade" id="@Dados.id" tabindex="-1">
    <div class="modal-dialog">
