                    </div>
                </div>
                <div class="skills progress-bar1">
                    @foreach (var Dados in Model.S8_Estatisticas as IEnumerable<Projeto_Laboral.Models.estatistica>)
                    {
                        if(Dados.id % 2 == 0)
                        {
                    <ul class="col-md-6 col-sm-12 col-xs-12 wow fadeInLeft">
                        <li class="progress">
                            <div class="progress-bar" data-width="@Dados.percentagem">
                                @Dados.titulo @Dados.percentagem%
                            </div>
                        </li>
                    </ul>
                    }
                    else
                    {
                    <ul class="col-md-6 col-sm-12 col-xs-12 wow fadeInRight">
                        <li class="progress">
                            <div class="progress-bar" data-width="@Dados.percentagem">
                                @Dados.titulo @Dados.percentagem%
                            </div>
                        </li>
                    </ul>
                    }
                    }
                </div>


            </div>
        </div>
