    <section id="ctn_sec">
        <div class="container">
            <div class="row">
                <div class="col-lg-12 col-md-12 col-sm-12 col-xs12 ">
                    <div class="title_sec">
                        <h1>Contatos</h1>
                        <h2>Estamos disponíveis para tirar a sua dúvida. entre em contato conosco</h2>
                    </div>
                </div>
                <div class="col-sm-6">
                    <div id="cnt_form">
                        @using (Html.BeginForm("Enviar_Email", "Home", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
                            <div class="form-group">
                                <input type="text" name="nome" class="form-control name-field" required="required" maxlength="35" placeholder="Nome">
                            </div>
                            <div class="form-group">
                                <input type="email" name="email" class="form-control mail-field" maxlength="46" required="required" placeholder="Email">
                            </div>
                            <div class="form-group">
                                <input type="number" name="contato" class="form-control mail-field" maxlength="11" required="required" placeholder="(81) 00000-0000">
                            </div>
                            <div class="form-group">
                                <textarea name="mensagem" required="required" class="form-control" rows="8" maxlength="300" placeholder="Massagem"></textarea>
                            </div>
                            <div class="form-group">
                                <button type="submit" class="btn btn-primary">Enviar</button>
                            </div>
}
                    </div>
                </div>
                <div class="col-lg-6 col-md-6 col-sm-6">
                    <div class="cnt_info">
                        <ul>
                            <li><i class="fa fa-envelope"></i><p>prolabore@prolaboresaude.com.br</p></li>
                            <li><i class="fa fa-phone"></i><p>(081) 99720-5799</p></li>
                            <li><i class="fa fa-phone"></i><p>(081) 99740-7169</p></li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </section>
