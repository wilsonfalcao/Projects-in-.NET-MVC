        {
        <p>@Dados.text</p>
        }
    </span>
    </a>
    </span>
        </div>
                             <div class="modal fade" id="@Dados.id" tabindex="-1">
    <div class="modal-dialog">
        <div class="modal-content">

            <iframe style="width:100%;height:80%;" src="@Dados.video"></iframe>
