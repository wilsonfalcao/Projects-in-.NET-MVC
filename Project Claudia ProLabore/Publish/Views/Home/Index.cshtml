            <img src="@Dados.img" style="width:100%;" alt="" />
        <span class="gallery_item_preview">
            <a>
                <div class="font_modal">
                    <h3>@Dados.title</h3>
                    <p>@Dados.text</p>
                </div>
            </a>
        </span>
    </div>
    </div>
    </div>
                            }
                            else if (Dados.type == 3)
                            {
