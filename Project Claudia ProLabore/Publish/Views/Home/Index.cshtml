    <script>
    function teste() {
        var div = null;
        $.ajax({
            dataType: "json",
            type: "GET",
            url: "Home/Carregar_Part",
            success: function (dados) {
                $(dados).each(function (i) {
                    div = document.getElementById("divID");

                    if (dados[i].type == 2 || dados[i].type == 1)
                    {
